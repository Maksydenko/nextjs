{
  "extends": [
    "stylelint-config-recess-order",
    "stylelint-config-recommended-scss"
  ],
  "rules": {
    "scss/operator-no-newline-after": null,
    "block-no-empty": null,
    "no-empty-source": null,
    "selector-pseudo-class-no-unknown": [
      true,
      {
        "ignorePseudoClasses": ["global"]
      }
    ]
  }
}
